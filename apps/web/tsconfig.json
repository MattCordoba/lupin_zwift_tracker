{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": [
      "node"
    ],
    "allowJs": true,
    "isolatedModules": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "next-env.d.ts",
    "pages",
    "styles"
  ],
  "exclude": [
    "node_modules"
  ]
}
